{
  "name": "pipboy",
  "version": "0.0.7",
  "description": "Experimental pipboy desktop and cli tool for Fallout 4",
  "main": "main.js",
  "bin": {
    "pipboy": "./cli.js"
  },
  "scripts": {
    "test": "echo \"Tests are cool, we should do those.\"",
    "start": "electron main.js",
    "build": "npm run build:osx && npm run build:win32 && npm run build:linux",
    "build:osx": "electron-packager ./ PipBoy --platform=darwin --arch=x64 --version=0.35.1",
    "build:win32": "electron-packager ./ PipBoy --platform=win32 --arch=x64 --version=0.35.1",
    "build:linux": "electron-packager ./ PipBoy --platform=linux --arch=x64 --version=0.35.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgbkrk/pipboy.git"
  },
  "keywords": [
    "Fallout4",
    "pipboy"
  ],
  "author": "Kyle Kelley <rgbkrk@gmail.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/rgbkrk/pipboy/issues"
  },
  "homepage": "https://github.com/rgbkrk/pipboy#readme",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-runtime": "^5.8.34",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.35.1"
  },
  "dependencies": {
    "color": "^0.10.1",
    "fluorine-lib": "^0.1.0",
    "history": "^1.13.1",
    "invariant": "^2.2.0",
    "json-loader": "^0.5.4",
    "pipboylib": "^1.1.1",
    "radium": "^0.15.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "rx": "^4.0.7"
  }
}
